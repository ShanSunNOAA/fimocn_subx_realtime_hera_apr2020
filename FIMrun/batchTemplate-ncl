# Create output directories
  conusdir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/130
  ${MKDIR} -p ${conusdir}
  t1dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t1
  ${MKDIR} -p ${t1dir}
  t2dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t2
  ${MKDIR} -p ${t2dir}
  t3dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t3
  ${MKDIR} -p ${t3dir}
  t4dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t4
  ${MKDIR} -p ${t4dir}
  t5dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t5
  ${MKDIR} -p ${t5dir}
