                sfc_totp.png    \
                12h_totp.png    \
                24h_totp.png    \
                sfc_acp.png     \
                esbl_hvyacp.png \
                sfc_acpcp.png   \
                sfc_weasd.png   \
                sfc_acsnw.png   \
                sfc_totsn.png   \
                12h_totsn.png   \
                24h_totsn.png   \
                ua_ceil.png     \
                ua_ctop.png     \
                sfc_rhpw.png    \
                int_PM25.png    \
