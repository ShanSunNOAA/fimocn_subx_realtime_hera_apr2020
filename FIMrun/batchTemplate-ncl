  ${MKDIR} -p ${t6dir}
  t7dir=${FIM_RUN}/${yyyymmddhh}/ncl_${MEMBER_ID}/t7
  ${MKDIR} -p ${t7dir}

# Copy png files to their proper names
  i=0
